@native
def readfile(fname: str) -> str:
    ccode """size_t ln;
    int err;
    char *out = yk__bhalib_read_file(yy__fname, &ln, &err);
    yk__sdsfree(yy__fname); // clean up filename as it will be copied
    if (err == 0) { return yk__sdsnewlen(out, ln); }
    return yk__sdsempty()"""

@native
def is_empty_str(s: str) -> bool:
    ccode """bool x = yk__sdslen(yy__s) == 0;
    yk__sdsfree(yy__s);
    return x"""

