    ccode """bool x = yk__sdslen(nn__s) == 0;
    yk__sdsfree(nn__s);
    return x"""

@native
def startswith(a: str, b: str) -> bool:
    ccode """bool x = (strstr(nn__a, nn__b) != NULL);
    yk__sdsfree(nn__a);
    yk__sdsfree(nn__b);
